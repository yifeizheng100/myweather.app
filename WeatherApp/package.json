{
  "name": "react-weather-webapp",
  "version": "1.0.0",
  "description": "Weather Broadcast Page with React. Only for Munich :)",
  "main": "dist/index.js",
  "homepage": "https://github.com/yifeizheng100/myweather.app.git",
  "dependencies": {
    "@material-ui/core": "3.9.2",
